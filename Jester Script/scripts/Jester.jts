
(defn integrate [function beg end]
	(set delta 0.001)
	(set x (float beg))
	(set area 0.0)
	
	(loop (< x end)
		(+= area (* delta (function x)))
		(+= x delta))
		
	area)

(defn bizbuz [range]
	(let [n 0]
		(loop (< n range)
			(print n "-")
			(if (= (mod n 3) 0) (print "bizz") nil)
			(if (= (mod n 5) 0) (print "buzz") nil)
			(println "")
			(+= n 1))))
		
	