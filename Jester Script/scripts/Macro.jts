
(defm lcomp (for var in list conditional test)
	
	(set result nil)
	
	(iterate var list
		(conditional test
			(append var result)
			nil))
	
	result)
	
(defm foreach (var in list action)
	(set new-list nil)
	
	(iterate var list
		(... action)
		(append var new-list))
	
	new-list)
	
(defn fac (n)
	(if (equal n 1)
		1
		(* n (fac (- n 1)))))