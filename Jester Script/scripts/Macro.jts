;examples of recursion timmerr

(function fac (n)
	(if (= n 0)
		1
		(* n (fac (- n 1)))))
		
(function fib (n)
	(if (<= n 1)
		n
		(+ (fib (- n 1)) (fib (- n 2)))))

(macro time (action)
	(reset)
	
	(set result action)
	
	(set duration (elapsed))
	
	(+ "result of action was " result " and took " duration))