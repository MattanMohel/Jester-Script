
(fn guesser (min max)
	
	(print "guess number between " min " and " max)
	
	(set user-in (string-int (input)))
	(set rand-in (random min max))
	
	(if (= user-in rand-in) nil
		(if (< user-in rand-in) (println "guess lower")
			(println "guess higher")))
	
)

