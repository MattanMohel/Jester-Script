
(macro intg (fun var lower upper delta)
	(set area 0.0)
	(set var (float lower))
	
	(loop (< var upper)
		(+= area (* delta fun))
		(+= var delta))
	
	area)
	
(macro lim (fun var point delta)
	(set var (float point))
	(set y1 fun)
		
	(+= var delta)
	(set y2 fun)
	
	(/ (- y2 y1) delta))
	
(fn reverse (lst)
	(set lst-size (size lst))
	(set revrs ( ))
	
	(loop (> lst-size 0)
		(append (nth (- lst-size 1) lst) revrs)
		(-- lst-size))
	
	revrs)

(type vec2
	(set x 0) 
	(set y 0))


	
